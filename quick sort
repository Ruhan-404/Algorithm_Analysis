
int pivote(vector<int>&v,int l,int hi)
{
	int pi = v[hi];
	int i = l;

	for(int j = l;j<hi;j++)
	{
		if(v[j]<pi)
		{
			swap(v[i],v[j]);
			i++;
		}
	}
	swap(v[i],v[hi]);
	return i;
}

void quick_sort(vector<int>&v,int l,int hi)
{
	if(l>=hi)return;

	int pi = pivote(v,l,hi);
	quick_sort(v,l,pi-1);
	quick_sort(v,pi+1,hi);
}

